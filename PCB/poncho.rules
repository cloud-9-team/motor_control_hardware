
(rules PCB poncho
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 750)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule In1.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle In1.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu)
    )
  )
  (class "kicad_default"
    /GPIO1 "/TEC_COL1" "/ENG0_OUT0" GND "/ENG0_OUT1" "/TEC_F3" +9V /GPIO2
    "/TEC_COL0" "/ENG1_OUT1" "/ENG1_OUT0" "/TEC_F2" +5V +3V3 /RESET /ISP
    /WAKEUP GNDA "/ADC0_3" "/ADC0_2" "/ADC0_1" /DAC "/VDD_A" "/I2C_SDA"
    "/I2C_SCL" "/RS232_RXD" "/RS232_TXD" "/CAN_RD" "/CAN_TD" "/TEC_F0" "/TEC_COL2" "/TEC_F1"
    "/ENET_RXD1" "/ENET_TX_EN" "/ENET_MDC" "/ENET_RXD0" "/ENET_CRS_DV" "/ENET_MDIO" "/ENET_TXD0" "/ENET_REF_CLK"
    "/ENET_TXD1" "/SPI_MISO" "/SPI_SCK" "/SPI_MOSI" /LCD4 "/LCD_EN" "/LCD_RS" /LCD3
    /LCD2 /GPIO0 /LCD1 /GPIO4 /GPIO3 /GPIO6 /GPIO5 /GPIO7
    /GPIO8
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu)
    )
  )
)